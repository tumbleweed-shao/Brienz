<Window x:Class="Brienz.Windows.LearntFromIt"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="304" d:DesignWidth="547">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="24*" />
            <RowDefinition Height="242*" />
            <RowDefinition Height="34*" />
        </Grid.RowDefinitions>
        <TextBlock Name="textBlock1" Text="TextBlock" Grid.Row="0" />
        <DockPanel Grid.Row="2" Name="dockPanel1">
            <Button Content="添加" Height="23" Name="AddLearnt" Width="75" />
            <Button Content="清除" Height="23" Name="button1" Width="75" />
            <Button Content="取消" Height="23" Name="CloseLearnt" Width="75" />
        </DockPanel>
        <StackPanel Grid.Row="1" Name="stackPanel1">
            <RichTextBox Name="richTextBox1" />
        </StackPanel>
    </Grid>
</Window>
